#: simple-photo-gallery/admin/wppg-photo-settings-menu.php:122
msgid "By ID Descending"
msgstr "Po opadajućem ID-ju"

#: simple-photo-gallery/admin/wppg-gallery-menu.php:388
#: simple-photo-gallery/admin/wppg-photo-settings-menu.php:123
msgid "By Date Ascending"
msgstr "Po rastućem datumu"

